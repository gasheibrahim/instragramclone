<% @favorites.each do |favorite| %>
<center>
<div class="card text-center" style="width:900px;border-radius:30px;">
  <div class="card-body">
    <h5 class="card-title"><%= favorite.user.fullname %></h5>
    <%= image_tag favorite.post.image.url, class:"upload" %><br>
    <%= link_to 'Back', posts_path, class:"btn" %>
</div><br>
<% end %>
</center>